#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a;
    char b;
    float c;
    double d;
    scanf("%c%d%f%lf",&b,&a,&c,&d);
    printf("%c\n%d\n%f\n%lf",b,a,c,d);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
