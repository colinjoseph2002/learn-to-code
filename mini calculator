#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
   float a,b;
   int c,d,e;
    float f;
    scanf("%f%f",&a,&b);
    c=a+b;
    d=a-b;
    e=a*b;
    f=a/b;
    printf("%d\n%d\n%d\n%.2f",c,d,e,f);
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
